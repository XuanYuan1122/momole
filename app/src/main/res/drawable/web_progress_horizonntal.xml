<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="1dip" />

            <gradient
                android:angle="270"
                android:centerColor="@color/pd_bg_center"
                android:centerY="0.75"
                android:endColor="@color/pd_bg_end"
                android:startColor="@color/pd_bg_start" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="1dip" />

                <gradient
                    android:angle="270"
                    android:centerColor="@color/pd_sec_progress_center"
                    android:centerY="0.75"
                    android:endColor="@color/pd_sec_progress_end"
                    android:startColor="@color/pd_sec_progress_start" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="1dip" />

                <gradient
                    android:angle="270"
                    android:centerColor="@color/web_progress_bar_color"
                    android:centerY="0.75"
                    android:endColor="@color/web_progress_bar_color"
                    android:startColor="@color/web_progress_bar_color" />
            </shape>
        </clip>
    </item>

</layer-list>
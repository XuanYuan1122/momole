<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/white">

        <com.moemoe.lalala.view.widget.view.DocLabelView
            android:id="@+id/dv_doc_label_root"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/size_18"
            android:paddingBottom="@dimen/size_18"
            android:layout_marginLeft="@dimen/size_18"
            android:layout_marginRight="@dimen/size_18"
            app:child_view_height="24dp"
            app:child_view_width="wrap_content"
            app:child_view_min_width="10dp"
            app:line_spacing="7dp"
            app:label_spacing="7dp">
        </com.moemoe.lalala.view.widget.view.DocLabelView>
</LinearLayout>
